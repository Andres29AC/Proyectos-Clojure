{
  "name": "landing-page",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "shadow-cljs": "3.2.0",
    "tailwindcss": "^4.1.13"
  },
  "scripts": {
    "watch:cljs": "shadow-cljs watch app",
    "build:cljs": "shadow-cljs release app",
    "build:css": "tailwindcss -i ./src/css/input.css -o ./public/css/global.css",
    "watch:css": "tailwindcss -i ./src/css/input.css -o ./public/css/global.css --watch",
    "dev": "npm-run-all -p watch:cljs watch:css",
    "build": "npm-run-all build:css build:cljs"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.8"
  }
}
